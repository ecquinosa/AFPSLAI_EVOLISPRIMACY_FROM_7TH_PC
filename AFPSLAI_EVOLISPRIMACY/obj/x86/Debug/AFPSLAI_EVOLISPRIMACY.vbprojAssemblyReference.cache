using System.Collections.Generic;
using System.Text;
using System.Runtime.InteropServices;

namespace ElySCardDotNet
{
    public class SCardManager : ISCardManager
    {
        private UInt32 m_hContext = 0;
        private int m_nLastError = 0;

        /// <summary>
        /// SCardManager constructor.
        /// </summary>
        public SCardManager()
        {
        }

        /// <summary>
        /// Wraps the PCSC function SCardEstablishContext. This function establishes the resource manager context (the scope) within which database operations are performed.
        /// </summary>
        /// <param name="Scope">Specifies the scope of the resource manager context.</param>
        public void EstablishContext(SCOPE dwScope)
        {
            IntPtr hContext = IntPtr.Zero;

            try
            {
                hContext = Marshal.AllocHGlobal(Marshal.SizeOf(m_hContext));

                m_nLastError = WinSCardNative._SCardEstablishContext((uint)dwScope, IntPtr.Zero, IntPtr.Zero, hContext);
                if (m_nLastError != 0)
                {
                    string msg = "SCardEstablishContext error: " + m_nLastError;

                    throw new SCardException(msg, m_nLastError);
                }

                m_hContext = (uint)Marshal.ReadInt32(hContext);
            }
            catch
            {
                throw;
            }
            finally
            {
                Marshal.FreeHGlobal(hContext);
            }
        }

        /// <summary>
        /// Wraps the PCSC function SCardReleaseContext. This function closes an established resource manager context, freeing any resources allocated under that context.
        /// </summary>        
        public void ReleaseContext()
        {
            if (m_hContext != 0)
            {
                m_nLastError = WinSCardNative._SCardReleaseContext(m_hContext);
                if (m_nLastError != 0)
                {
                    string msg = "SCardReleaseContext error: " + m_nLastError;

                    throw new SCardException(msg, m_nLastError);
                }

                m_hContext = 0;
            }
        }

        /// <summary>
        /// Wraps the PCSC funciton SCardListReaders. This function provides the list of readers.
        /// </summary>
        /// <returns>A string array that list the card readers.</returns>
        public string[] ListReaders()
        {
            string[] sListReaders = null;
            string sReaders;
            UInt32 pchReaders = 0;
            IntPtr pszListReaders = IntPtr.Zero;

            //pchReaders = SCARD_AUTOALLOCATE;
            //m_nLastError = WinSCardNative.SCardListReaders(m_hContext, IntPtr.Zero, out baListReaders, ref pchReaders);

            m_nLastError = WinSCardNative._SCardListReaders(m_hContext, IntPtr.Zero, pszListReaders, ref pchReaders);
            if (m_nLastError != 0)
            {
                string msg = "SCardListReaders error: " + m_nLastError;

                throw new SCardException(msg, m_nLastError);
            }
            else
            {
                try
                {
                    pszListReaders = Marshal.AllocHGlobal((int)(pchReaders * 2));
                    m_nLastError = WinSCardNative._SCardListReaders(m_hContext, IntPtr.Zero, pszListReaders, ref pchReaders);
                    if (m_nLastError == 0)
                    {
                        // Copy the string to managed memory and
                        // Remove the two '\0' last characters indicating the end of string
                        sReaders = Marshal.PtrToStringAnsi(pszListReaders, (int)(pchReaders - 2));

                        // Next we split up the null delimited strings into a string array
                        char[] delimiter = new char[1];
                        delimiter[0] = Convert.ToChar(0);

                        sListReaders = sReaders.Split(delimiter);
                        Array.Sort(sListReaders);
                    }
                }
                catch
                {
                    throw;
                }
                finally
                {
                    Marshal.FreeHGlobal(pszListReaders);
                }
            }

            return sListReaders;
        }


        /// <summary>
        /// This function return a SCardConnection object associated with the specified reader.
        /// </summary>
        /// <param name="reader"></param>
        /// <returns>Specifies the card reader containing the smart card to use.</returns>
        public SCardConnection CreateConnection(string reader)
        {
            return new SCardConnection(m_hContext, reader);
        }


        /// <summary>
        /// Wraps the PCSC function SCardCancel. This function terminates all outstanding actions within a specific resource manager context.
        /// </summary>        
        public void Cancel()
        {
            m_nLastError = WinSCardNative._SCardCancel(m_hContext);
            if (m_nLastError != 0)
            {
                string msg = "SCardCancel error: " + m_nLastError;

                throw new SCardException(msg, m_nLastError);
            }
        }

        /// <summary>
        /// Wraps the PCSC function SCardGetStatusChange. This function blocks execution until the current availability of the cards in a specific set of readers changes.
        /// </summary>
        /// <param name="dwTimeout">Specifies the maximum amount of time, in milliseconds, to wait for an action. A value of zero causes the function to return immediately. A value of INFINITE causes this function never to time out.</param>
        /// <param name="rgReaderStates">Supplies an array of SCARD_READERSTATE structures that specify the readers to watch, and receives the result.</param>
        /// <returns>If the timeout has expired the function returns the value SCARD_E_TIMEOUT.</returns>
        public long GetStatusChange(int dwTimeout, SCARD_READERSTATE[] readersState)
        {
            IntPtr rgReaderState = IntPtr.Zero;

            try
            {
                uint cReaders = (uint)readersState.Length;

                int size = Marshal.SizeOf(readersState[0]);
                rgReaderState = Marshal.AllocHGlobal(readersState.Length * size);
                int pos = (int)rgReaderState;
                for (int i = 0; i < cReaders; i++)
                {
                    Marshal.StructureToPtr(readersState[i], (IntPtr)pos, false);
                    pos += size;
                }

                m_nLastError = WinSCardNative._SCardGetStatusChange(m_hContext, unchecked((uint)dwTimeout),
                    rgReaderState, cReaders);
                if (m_nLastError == SCardException.SCARD_E_TIMEOUT)
                {
                    return m_nLastError;
                }
                else if (m_nLastError != 0 && m_nLastError != SCardException.SCARD_E_NO_READERS_AVAILABLE)
                {
                    string msg = "SCardGetStatusChange error: " + m_nLastError;

                    throw new SCardException(msg, m_nLastError);
                }

                pos = (int)rgReaderState;
                for (int i = 0; i < cReaders; i++)
                {
                    readersState[i] = (SCARD_READERSTATE)Marshal.PtrToStructure((IntPtr)pos,
                        typeof(SCARD_READERSTATE));
                    pos += size;
                }
            }
            catch
            {
                throw;
            }
            finally
            {
                Marshal.FreeHGlobal(rgReaderState);
            }

            return 0;
        }
    }
}
