		void Connect(SHARE ShareMode, PROTOCOL PreferredProtocols);

		/// <summary>
        /// Wraps the PCSC function SCardDisconnect. This function terminates a connection previously opened with a smart card.
		/// </summary>
        /// <param name="Disposition">Specifies what to do with the card in the connected reader on close.</param>
        void Disconnect(DISCONNECT Disposition);

        /// <summary>
        /// Wraps the PCSC function SCardReconnect. This function re-establishes an existing connection between the calling application and a smart card.
        /// </summary>
        /// <param name="ShareMode">Specifies a flag that indicates whether other applications can form connections to the card.</param>
        /// <param name="PreferredProtocols">Specifies a bit mask of acceptable protocols for the connection.</param>
        /// <param name="Initialization">Specifies the kind of initialization to perform on the card.</param>
        void Reconnect(SHARE ShareMode, PROTOCOL PreferredProtocols, DISCONNECT Initialization);


        /// <summary>
        /// Gets a value indicating whether a smartcard is connected.
        /// </summary>
        bool Connected{ get; }


        /// <summary>
        /// Wraps the PCSC function SCardStatus. This function provides the current status of a smart card in a reader.
        /// </summary>
        /// <param name="szReaderName">Receives a list of friendly names (multi-string) by which the currently connected reader is known.</param>
        /// <param name="State">Pointer that specifies the current state of the smart card in the reader.</param>
        /// <param name="Protocol">Pointer that specifies the current protocol, if any. The returned value is meaningful only if the returned value of pdwState is SCARD_SPECIFICMODE.</param>
        byte[] GetStatus(out string[] szReaderName, out STATE State, out PROTOCOL Protocol);

		/// <summary>
        /// Wraps the PCSC function SCardTransmit. This function sends a service request to the smart card, and receives data back from the card.
		/// </summary>
        /// <param name="abSendBuffer">Byte array containing the APDU command.</param>
        /// <param name="cbSendLength">Length of the APDU command.</param>
        /// <returns>Byte array that contains the response from the card</returns>
        byte[] Transmit(byte[] abSendBuffer, UInt32 cbSendLength);

        /// <summary>
        /// Wraps the PCSC function SCardTransmit. It sends a service request to the smart card, and receives data back from the card.
        /// Use this function to receive Extended APDU. 
        /// </summary>
        /// <param name="abSendBuffer">Byte array containing the APDU command.</param>
        /// <param name="cbSendLength">Length of the APDU command.</param>
        /// <param name="pcbRecvLength">Supplies the length, in bytes, of the expected response.</param>
        /// <returns>Byte array that contains the response from the card</returns>
        byte[] TransmitEx(byte[] abSendBuffer, UInt32 cbSendLength, UInt32 pcbRecvLength);

        /// <summary>
        /// Wraps the PCSC function SCardControl. This function gives you direct control of the reader.
        /// </summary>
        /// <param name="dwControlCode">Specifies the control code for the operation. This value identifies the specific operation to be performed.</param>
        /// <param name="lpInBuffer">Byte array that contains the data required to perform the operation. This parameter can be NULL if the dwControlCode parameter specifies an operation that does not require input data.</param>
        /// <param name="nInBufferSize">Specifies the size, in bytes, of the buffer lpInBuffer.</param>
        /// <param name="nOutBufferSize">Specifies the size, in bytes, of the buffer lpOutBuffer.</param>
        /// <returns>Returns a byte array that receives the operation's output data. This parameter can be NULL if the dwControlCode parameter specifies an operation that does not produce output data.</returns>
        byte[] Control(UInt32 dwControlCode, byte[] lpInBuffer, UInt32 nInBufferSize, UInt32 nOutBufferSize);

        /// <summary>
        /// Wraps the PCSC function SCardBeginTransaction. This function starts a transaction, waiting for the completion of all other transactions before it begins.
        /// </summary>
        void BeginTransaction();

        /// <summary>
        /// Wraps the PCSC function SCardEndTransaction. This function completes a previously declared transaction, enabling other applications to resume interactions with the card.
        /// </summary>
        /// <param name="Disposition">Specifies the action that occurs when the connected reader closes.</param>
        void EndTransaction(DISCONNECT Disposition);

	}

    public	enum SHARE
	{
        /// <summary>
        /// This application is not willing to share this
        /// card with other applications.
        /// </summary>
		Exclusive = 1,
        /// <summary>
        /// This application is willing to share this
        /// card with other applications.
        /// </summary>
		Shared,
		/// <summary>
        /// This application demands direct control of
        /// the reader, so it is not available to other
        /// applications.
		/// </summary>
		Direct
	}

	public enum PROTOCOL
	{
        /// <summary>
        /// There is no active protocol.
        /// </summary>
		Undefined	= 0x00000000,
        /// <summary>
        /// T=0 is the active protocol.
        /// </summary>
		T0			= 0x00000001,
        /// <summary>
        /// T=1 is the active protocol.
        /// </summary>
		T1			= 0x00000002,
        /// <summary>
        /// Raw is the active protocol.
        /// </summary>
		Raw			= 0x00010000,
        /// <summary>
        /// Use implicit PTS.
        /// </summary>
		Default		= unchecked ((int) 0x80000000),
		T0orT1		= T0 | T1
    }

    public enum STATE
    {
        /// <summary>
        /// This value implies the driver is unaware of the current state of the reader.
        /// </summary>
        SCARD_UNKNOWN = 0x00000000,
        /// <summary>
        /// There is no card in the card reader.
        /// </summary>
        SCARD_ABSENT = 0x00000001,
        /// <summary>
        /// A card is present in the card reader, but it is not in position for use.
        /// </summary>
        SCARD_PRESENT = 0x00000002,
        /// <summary>
        /// A card is in the reader and is in position for use. The card is not powered.
        /// </summary>
        SCARD_SWALLOWED = 0x00000003,
        /// <summary>
        /// The card is powered but the reader cannot determine its mode.
        /// </summary>
        SCARD_POWERED = 0x00000004,
        /// <summary>
        /// The card has been reset and is waiting for protocol negotiation.
        /// </summary>
        SCARD_NEGOTIABLE = 0x00000005,
        /// <summary>
        /// The card has been reset and a protocol has been negotiated.
        /// </summary>
        SCARD_SPECIFIC = 0x00000006
    }

	public enum DISCONNECT
	{
        /// <summary>
        /// Don't do anything special on close
        /// </summary>
		Leave,
        /// <summary>
        /// Reset the card on close
        /// </summary>
		Reset,
        /// <summary>
        /// Power down the card on close
        /// </summary>
		Unpower,
        /// <summary>
        /// Eject the card on close
        /// </summary>
		Eject
	}
}
