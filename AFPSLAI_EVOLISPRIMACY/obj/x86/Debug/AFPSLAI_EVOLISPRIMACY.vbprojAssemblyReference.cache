        [DllImport("ElyRdrControl.dll", EntryPoint = "RDR_WriteCTLRegister")]
        internal static extern Int32 _RDR_WriteCTLRegister(byte byAddress, byte byValue);

        [DllImport("ElyRdrControl.dll", EntryPoint = "RDR_ListReaders")]
        internal static extern Int32 _RDR_ListReaders([MarshalAs(UnmanagedType.LPArray, SizeConst=160)] byte[] lpSNList,
            ref UInt32 lpSNListSize);

        [DllImport("ElyRdrControl.dll", EntryPoint = "RDR_OpenCommEx")]
        internal static extern Int32 _RDR_OpenCommEx([MarshalAs(UnmanagedType.LPArray, SizeConst=32)]byte[] lpSN);

        [DllImport("ElyRdrControl.dll", EntryPoint = "RDR_SetContactNegotiationSpeed")]
        internal static extern Int32 _RDR_SetContactNegotiationSpeed(byte byFD);


        internal const int MAX_CCID_DATA_SIZE = 254;

        public static void RDR_OpenComm()
        {
            int iResult = _RDR_OpenComm();
            if (iResult != 0)
                throw new ElyRDRException(iResult);
        }

        public static void RDR_CloseComm()
        {
            int iResult = _RDR_CloseComm();
            //if (iResult != 0)
            //    throw new ElyRDRException(iResult);
        }

        public static byte[] RDR_SendCommand(byte byMessageType, byte bySlot, byte[] lpInputBuffer)
        {
            byte[] lpOutputBuffer = new byte[MAX_CCID_DATA_SIZE];
            uint lpBytesReturned = MAX_CCID_DATA_SIZE;
            int iResult = _RDR_SendCommand(byMessageType, bySlot, lpInputBuffer, (uint)lpInputBuffer.Length,
                lpOutputBuffer, ref lpBytesReturned);

            if (iResult != 0)
                throw new ElyRDRException(iResult);

            return lpOutputBuffer;
        }

        public static void RDR_GetFirmwareVersion(ref byte byMajor, ref byte byMinor)
        {
            int iResult = _RDR_GetFirmwareVersion(ref byMajor, ref byMinor);
            if (iResult != 0)
                throw new ElyRDRException(iResult);
        }

        public static string RDR_GetSerialNumber()
        {
            byte[] lpSerialNumber = new byte[32];
            uint uiSNSize = (uint)lpSerialNumber.Length;
            int iResult = _RDR_GetSerialNumber(lpSerialNumber);
            if (iResult != 0)
                throw new ElyRDRException(iResult);

            return Encoding.ASCII.GetString(lpSerialNumber, 0, 32);
        }

        public static void RDR_SetRF( bool bActive )
        {
            int iResult = _RDR_SetRF(bActive);
            if (iResult != 0)
                throw new ElyRDRException(iResult);
        }

        public static void RDR_StopPolling()
        {
            int iResult = _RDR_StopPolling();
            if (iResult != 0)
                throw new ElyRDRException(iResult);
        }

        public static void RDR_StartPolling()
        {
            int iResult = _RDR_StartPolling();
            if (iResult != 0)
                throw new ElyRDRException(iResult);
        }

        public static byte[] RDR_GetStaticRFParams()
        {
            byte [] RFParams = new byte[128];
            uint uiRFParamsSize = (uint) RFParams.Length;
            int iResult = _RDR_GetStaticRFParams(RFParams, ref uiRFParamsSize);
            if (iResult != 0)
                throw new ElyRDRException(iResult);
            if( uiRFParamsSize != 128 )
                throw new ElyRDRException("Unexpected byte array size!", SCardException.SCARD_F_UNKNOWN_ERROR);

            return RFParams;
        }

        public static void RDR_SetStaticRFParams( byte[] RFParams )
        {
            int iResult = _RDR_SetStaticRFParams(RFParams, (uint)RFParams.Length);
            if (iResult != 0)
                throw new ElyRDRException(iResult);
        }

        public static byte[] RDR_GetDynamicRFParams()
        {
            byte[] RFParams = new byte[128];
            uint uiRFParamsSize = (uint)RFParams.Length;
            int iResult = _RDR_GetDynamicRFParams(RFParams, ref uiRFParamsSize);
            if (iResult != 0)
                throw new ElyRDRException(iResult);
            if (uiRFParamsSize != 128)
                throw new ElyRDRException("Unexpected byte array size!", SCardException.SCARD_F_UNKNOWN_ERROR);

            return RFParams;
        }

        public static void RDR_SetDynamicRFParams(byte[] RFParams)
        {
            int iResult = _RDR_SetDynamicRFParams(RFParams, (uint)RFParams.Length);
            if (iResult != 0)
                throw new ElyRDRException(iResult);
        }

        public static string[] RDR_ListReaders()
        {
            uint iSize = 5;
            byte[] lpSNList = new byte[160];
            int iResult = _RDR_ListReaders(lpSNList, ref iSize);
            if (iResult != 0)
                throw new ElyRDRException(iResult);

            string [] strSNList = new string[iSize];
            for (int i = 0; i < iSize; ++i)
            {
                strSNList[i] = Encoding.ASCII.GetString(lpSNList, i * 32, 32);
            }

            return strSNList;
        }

        public static void RDR_OpenComm(string strSN)
        {
            byte [] bySN = ASCIIEncoding.ASCII.GetBytes(strSN);
            if( bySN == null || bySN.Length != 32)
                throw new FormatException("The format of the serial number is incorrect.");

            int iResult = _RDR_OpenCommEx(bySN);
            if (iResult != 0)
                throw new ElyRDRException(iResult);
        }

        public static void RDR_SetContactNegotiationSpeed(byte byFD)
        {
            int iResult = _RDR_SetContactNegotiationSpeed(byFD);
            if (iResult != 0)
                throw new ElyRDRException(iResult);
        }
    }
