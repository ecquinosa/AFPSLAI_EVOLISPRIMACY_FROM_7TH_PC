        /// <param name="hContext">Specifies the SCardContext handle.</param>
        /// <param name="reader">Specifies the name of the reader containing the target card.</param>
        public SCardConnection(UInt32 hContext, string reader)
        {
            m_hContext = hContext;
            m_reader = reader;
            m_connected = false;
        }


        /// <summary>
        ///  Wraps the PCSC function SCardConnect. This function establishes a connection with a smart card.
        /// </summary>
        /// <param name="ShareMode">Specifies a flag that indicates whether other applications can form connections to the card.</param>
        /// <param name="PreferredProtocols">Specifies a bit mask of acceptable protocols for the connection.</param>
        public void Connect(SHARE ShareMode, PROTOCOL PreferredProtocols)
        {
            IntPtr hCard = IntPtr.Zero;
            IntPtr pProtocol = IntPtr.Zero;

            try
            {
                // Allocate the memory to receive the pointer's data
                hCard = Marshal.AllocHGlobal(Marshal.SizeOf(m_hCard));
                pProtocol = Marshal.AllocHGlobal(Marshal.SizeOf(m_nProtocol));

                m_nLastError = WinSCardNative._SCardConnect(m_hContext, m_reader,
                    (uint)ShareMode, (uint)PreferredProtocols, hCard, pProtocol);

                if (m_nLastError != 0)
                {
                    string msg = "SCardConnect error: " + m_nLastError;

                    throw new SCardException(m_nLastError);
                }

                m_hCard = (uint)Marshal.ReadInt32(hCard);
                m_nProtocol = (uint)Marshal.ReadInt32(pProtocol);
                m_connected = true;
            }
            catch
            {
                throw;
            }
            finally
            {
                Marshal.FreeHGlobal(hCard);
                Marshal.FreeHGlobal(pProtocol);
            }
        }


        /// <summary>
        /// Wraps the PCSC function SCardDisconnect. This function terminates a connection previously opened with a smart card.
        /// </summary>
        /// <param name="Disposition">Specifies what to do with the card in the connected reader on close.</param>
        public void Disconnect(DISCONNECT Disposition)
        {
            if (m_hCard != 0)
            {
                m_nLastError = WinSCardNative._SCardDisconnect(m_hCard, (uint) Disposition);

                if (m_nLastError != 0)
                {
                    string msg = "SCardDisconnect error: " + m_nLastError;
                    //throw new SCardException(msg, m_nLastError);
                    throw new SCardException(m_nLastError);
                }
                m_connected = false;
                m_hCard = 0;
            }
        }


        /// <summary>
        /// Wraps the PCSC function SCardReconnect. This function re-establishes an existing connection between the calling application and a smart card.
        /// </summary>
        /// <param name="ShareMode">Specifies a flag that indicates whether other applications can form connections to the card.</param>
        /// <param name="PreferredProtocols">Specifies a bit mask of acceptable protocols for the connection.</param>
        /// <param name="Initialization">Specifies the kind of initialization to perform on the card.</param>
        public void Reconnect(SHARE ShareMode, PROTOCOL PreferredProtocols, DISCONNECT Initialization)
        {
            IntPtr pProtocol = IntPtr.Zero;
            try
            {
                if (m_hCard != 0)
                {
                    // Allocate the memory to receive the pointer's data
                    pProtocol = Marshal.AllocHGlobal(Marshal.SizeOf(m_nProtocol));

                    m_nLastError = WinSCardNative._SCardReconnect(m_hCard, (uint)ShareMode, (uint)PreferredProtocols,
                        (uint)Initialization, pProtocol);

                    if (m_nLastError != 0)
                    {
                        string msg = "SCardReconnect error: " + m_nLastError;
                        throw new SCardException(m_nLastError);
                    }

                    m_nProtocol = (uint)Marshal.ReadInt32(pProtocol);
                    m_connected = true;
                }
            }
            catch
            {
                throw;
            }
            finally
            {
                Marshal.FreeHGlobal(pProtocol);
            }
        }


        /// <summary>
        /// Gets a value indicating whether a smartcard is connected.
        /// </summary>
        public bool Connected
        {
            get { return m_connected; }
        }private bool m_connected;


        /// <summary>
        /// Wraps the PCSC function SCardStatus. This function provides the current status of a smart card in a reader.
        /// </summary>
        /// <param name="szReaderName">Receives a list of friendly names (multi-string) by which the currently connected reader is known.</param>
        /// <param name="State">Pointer that specifies the current state of the smart card in the reader.</param>
        /// <param name="Protocol">Pointer that specifies the current protocol, if any. The returned value is meaningful only if the returned value of pdwState is SCARD_SPECIFICMODE.</param>
        public byte[] GetStatus(out string[] sListReaders, out STATE State, out PROTOCOL Protocol)
        {
            UInt32 pcchReaderLen = WinSCardNative.SCARD_AUTOALLOCATE;
            IntPtr szReaderName = IntPtr.Zero;
            string sReaders;
            byte[] pbATR = new byte[33];
            UInt32 pbcAtrLen = 33;

            UInt32 pdwState, pdwProtocol;

            m_nLastError = WinSCardNative._SCardStatus(m_hCard, out szReaderName, ref pcchReaderLen,
                out pdwState, out pdwProtocol, pbATR, out pbcAtrLen);

            if (m_nLastError == 0)
            {
                State = (STATE)pdwState;
                Protocol = (PROTOCOL)pdwProtocol;

                // Copy the string to managed memory and
                // Remove the two '\0' last characters indicating the end of string
                sReaders = Marshal.PtrToStringUni(szReaderName, (int)(pcchReaderLen - 2));

                // Free allocated memory by SCARD_AUTOALLOCATE use
                WinSCardNative._SCardFreeMemory(m_hContext, szReaderName);

                // Next we split up the null delimited strings into a string array
                char[] delimiter = new char[1];
                delimiter[0] = Convert.ToChar(0);

                sListReaders = sReaders.Split(delimiter);
                Array.Sort(sListReaders);

                byte[] resizedATR = new byte[pbcAtrLen];
                Array.Copy(pbATR, resizedATR, (int)pbcAtrLen);
                return resizedATR;
            }
            else
            {
                string msg = "SCardStatus error: " + m_nLastError;
                throw new SCardException(m_nLastError);
            }
        }


        /// <summary>
        /// Wraps the PCSC function SCardTransmit. This function sends a service request to the smart card, and receives data back from the card.
        /// </summary>
        /// <param name="abSendBuffer">Byte array containing the APDU command.</param>
        /// <param name="cbSendLength">Length of the APDU command.</param>
        /// <returns>Byte array that contains the response from the card</returns>
        public byte[] Transmit(byte[] baSendBuffer, UInt32 cbSendLength)
        {
            IntPtr pioSendPci;
            
            // Maximun length for APDU response + 2 status bytes
            byte[] baRecvBuffer = new byte[258];
            uint pcbRecvLength = (uint) baRecvBuffer.Length;

            if (m_nProtocol == (uint)PROTOCOL.T0)
            {
                pioSendPci = WinSCardNative.SCARD_PCI_T0;
            }
            else if (m_nProtocol == (uint)PROTOCOL.T1)
            {
                pioSendPci = WinSCardNative.SCARD_PCI_T1;
            }
            else
            {
                pioSendPci = WinSCardNative.SCARD_PCI_RAW;
            }

            m_nLastError = WinSCardNative._SCardTransmit(m_hCard, pioSendPci, baSendBuffer, cbSendLength,
                IntPtr.Zero, baRecvBuffer, ref pcbRecvLength);

            if (m_nLastError != 0)
            {
                string msg = "SCardTransmit error: " + m_nLastError;
                throw new SCardException(m_nLastError);
            }


            byte[] baResizedArray = new byte[pcbRecvLength];
            Array.Copy(baRecvBuffer, baResizedArray, (int)pcbRecvLength);
            return baResizedArray;
        }


        /// <summary>
        /// Wraps the PCSC function SCardTransmit. It function sends a service request to the smart card, and receives data back from the card.
        /// Use this function to receive Extended APDU. 
        /// </summary>
        /// <param name="abSendBuffer">Byte array containing the APDU command.</param>
        /// <param name="cbSendLength">Length of the APDU command.</param>
        /// <param name="pcbRecvLength">Supplies the length, in bytes, of the expected response.</param>
        /// <returns>Byte array that contains the response from the card</returns>
        public byte[] TransmitEx(byte[] baSendBuffer, UInt32 cbSendLength, UInt32 pcbRecvLength)
        {
            IntPtr pioSendPci;

            byte[] baRecvBuffer = new byte[pcbRecvLength];

            if (m_nProtocol == (uint)PROTOCOL.T0)
            {
                pioSendPci = WinSCardNative.SCARD_PCI_T0;
            }
            else if (m_nProtocol == (uint)PROTOCOL.T1)
            {
                pioSendPci = WinSCardNative.SCARD_PCI_T1;
            }
            else
            {
                pioSendPci = WinSCardNative.SCARD_PCI_RAW;
            }

            m_nLastError = WinSCardNative._SCardTransmit(m_hCard, pioSendPci, baSendBuffer, cbSendLength,
                IntPtr.Zero, baRecvBuffer, ref pcbRecvLength);

            if (m_nLastError != 0)
            {
                string msg = "SCardTransmit error: " + m_nLastError;
                throw new SCardException(m_nLastError);
            }


            byte[] baResizedArray = new byte[pcbRecvLength];
            Array.Copy(baRecvBuffer, baResizedArray, (int)pcbRecvLength);
            return baResizedArray;
        }


        /// <summary>
        /// Wraps the PCSC function SCardControl. This function gives you direct control of the reader.
        /// </summary>
        /// <param name="dwControlCode">Specifies the control code for the operation. This value identifies the specific operation to be performed.</param>
        /// <param name="lpInBuffer">Byte array that contains the data required to perform the operation. This parameter can be NULL if the dwControlCode parameter specifies an operation that does not require input data.</param>
        /// <param name="nInBufferSize">Specifies the size, in bytes, of the buffer lpInBuffer.</param>
        /// <param name="nOutBufferSize">Specifies the size, in bytes, of the buffer lpOutBuffer.</param>
        /// <returns>Returns a byte array that receives the operation's output data. This parameter can be NULL if the dwControlCode parameter specifies an operation that does not produce output data.</returns>
        public byte[] Control(UInt32 dwControlCode, byte[] lpInBuffer, UInt32 nInBufferSize,
            UInt32 nOutBufferSize )
        {
            UInt32 lpBytesReturned;
            byte[] baOutBuffer = null;
            IntPtr lpOutBuffer = IntPtr.Zero;

            try
            {
                if (nOutBufferSize > 0)
                {
                    // Alloc memory for output buffer
                    lpOutBuffer = Marshal.AllocHGlobal((int)nOutBufferSize);
                }

                m_nLastError = WinSCardNative._SCardControl(m_hCard, dwControlCode, lpInBuffer, nInBufferSize,
                    lpOutBuffer, nOutBufferSize, out lpBytesReturned);

                if (m_nLastError != 0)
                {
                    string msg = "SCardControl error: " + m_nLastError;
                    //throw new SCardException(msg, m_nLastError);
                    throw new SCardException(m_nLastError);
                }

                if (lpBytesReturned > 0)
                {
                    baOutBuffer = new byte[lpBytesReturned];
                    Marshal.Copy(lpOutBuffer, baOutBuffer, 0, (int)lpBytesReturned);
                }
            }
            catch
            {
                throw;
            }
            finally
            {
                Marshal.FreeHGlobal(lpOutBuffer);
            }

            return baOutBuffer;
        }


        /// <summary>
        /// Wraps the PCSC function SCardBeginTransaction. This function starts a transaction, waiting for the completion of all other transactions before it begins.
        /// </summary>
        public void BeginTransaction()
        {
            m_nLastError = WinSCardNative._SCardBeginTransaction(m_hCard);
            
            if (m_nLastError != 0)
            {
                string msg = "SCardBeginTransaction error: " + m_nLastError;
                throw new SCardException(m_nLastError);
            }
        }


        /// <summary>
        /// Wraps the PCSC function SCardEndTransaction. This function completes a previously declared transaction, enabling other applications to resume interactions with the card.
        /// </summary>
        /// <param name="Disposition">Specifies the action that occurs when the connected reader closes.</param>
        public void EndTransaction(DISCONNECT Disposition)
        {
            m_nLastError = WinSCardNative._SCardEndTransaction(m_hCard, (uint) Disposition);

            if (m_nLastError != 0)
            {
                string msg = "SCardEndTransaction error: " + m_nLastError;
                throw new SCardException(m_nLastError);
            }
        }
